{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getRecipe, updateRecipe } from '../services/recipeService';\nexport default {\n  name: 'EditRecipe',\n  data() {\n    return {\n      recipe: {\n        title: '',\n        ingredients: [],\n        instructions: '',\n        cookingTime: 0,\n        difficulty: ''\n      },\n      newIngredient: ''\n    };\n  },\n  async created() {\n    const response = await getRecipe(this.$route.params.id);\n    this.recipe = response.data;\n  },\n  methods: {\n    async editRecipe() {\n      await updateRecipe(this.$route.params.id, this.recipe);\n      this.$router.push('/recipes');\n    },\n    addIngredient() {\n      if (this.newIngredient) {\n        this.recipe.ingredients.push(this.newIngredient);\n        this.newIngredient = '';\n      }\n    },\n    removeIngredient(index) {\n      this.recipe.ingredients.splice(index, 1);\n    }\n  }\n};","map":{"version":3,"names":["getRecipe","updateRecipe","name","data","recipe","title","ingredients","instructions","cookingTime","difficulty","newIngredient","created","response","$route","params","id","methods","editRecipe","$router","push","addIngredient","removeIngredient","index","splice"],"sources":["C:\\Users\\Owen\\Desktop\\Recipe FullStack\\frontend\\cook\\src\\views\\EditRecipe.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1>Edit Recipe</h1>\r\n    <form @submit.prevent=\"editRecipe\">\r\n      <div>\r\n        <label>Title:</label>\r\n        <input v-model=\"recipe.title\" required />\r\n      </div>\r\n      <div>\r\n        <label>Ingredients:</label>\r\n        <input v-model=\"newIngredient\" @keyup.enter=\"addIngredient\" />\r\n        <button @click.prevent=\"addIngredient\">Add</button>\r\n        <ul>\r\n          <li v-for=\"(ingredient, index) in recipe.ingredients\" :key=\"index\">\r\n            {{ ingredient }}\r\n            <button @click.prevent=\"removeIngredient(index)\">Remove</button>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n      <div>\r\n        <label>Instructions:</label>\r\n        <textarea v-model=\"recipe.instructions\" required></textarea>\r\n      </div>\r\n      <div>\r\n        <label>Cooking Time:</label>\r\n        <input v-model=\"recipe.cookingTime\" type=\"number\" required />\r\n      </div>\r\n      <div>\r\n        <label>Difficulty:</label>\r\n        <input v-model=\"recipe.difficulty\" required />\r\n      </div>\r\n      <button type=\"submit\">Update Recipe</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getRecipe, updateRecipe } from '../services/recipeService';\r\n\r\nexport default {\r\n  name: 'EditRecipe',\r\n  data() {\r\n    return {\r\n      recipe: {\r\n        title: '',\r\n        ingredients: [],\r\n        instructions: '',\r\n        cookingTime: 0,\r\n        difficulty: '',\r\n      },\r\n      newIngredient: '',\r\n    };\r\n  },\r\n  async created() {\r\n    const response = await getRecipe(this.$route.params.id);\r\n    this.recipe = response.data;\r\n  },\r\n  methods: {\r\n    async editRecipe() {\r\n      await updateRecipe(this.$route.params.id, this.recipe);\r\n      this.$router.push('/recipes');\r\n    },\r\n    addIngredient() {\r\n      if (this.newIngredient) {\r\n        this.recipe.ingredients.push(this.newIngredient);\r\n        this.newIngredient = '';\r\n      }\r\n    },\r\n    removeIngredient(index) {\r\n      this.recipe.ingredients.splice(index, 1);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAqCA,SAASA,SAAS,EAAEC,YAAW,QAAS,2BAA2B;AAEnE,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QACNC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,YAAY,EAAE,EAAE;QAChBC,WAAW,EAAE,CAAC;QACdC,UAAU,EAAE;MACd,CAAC;MACDC,aAAa,EAAE;IACjB,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAMC,QAAO,GAAI,MAAMZ,SAAS,CAAC,IAAI,CAACa,MAAM,CAACC,MAAM,CAACC,EAAE,CAAC;IACvD,IAAI,CAACX,MAAK,GAAIQ,QAAQ,CAACT,IAAI;EAC7B,CAAC;EACDa,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MAC